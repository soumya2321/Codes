/*sumof all odd numbers and sum of all even */



#include<stdio.h>
int main()
{
	int i;
	int evensum=0;
	int oddsum=0;
	for(i=0;i<=10;i++)
	{
		if(i%2==0)
		{
			evensum+=i;
		
		}
		else {
		oddsum+=i;
	    }
    }
	printf("total sum of even numbers= %d\n ",evensum);
	printf("total sum of odd numbers= %d\n",oddsum);
	return 0;
}
