//to check whether the number is prime or not
#include<stdio.h>

int main()
{
    int n,i,cnt=0;
    printf("enter n:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        cnt++;
    }
    if(cnt==2)
       printf("%d is prime number");
    else
       printf("%d is not prime number");
   return 0;
        
}




//to print prime number up to n and counting that numbers using function
#include<stdio.h>
#include<stdbool.h>

bool isprime(int n)
{
    int i,cnt = 0;
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
           cnt++;
    }
    if(cnt==2)
       return true;
    else
       return false;
   return 0;
}

int main()
{
    int n,count=0,i;
    printf("enter n:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
      if(isprime(i))
      {
         printf("%d ",i);
         count++;
      }
      printf("\ntotal=%d",count);
        
}

//to print all prime numbers upto n
int main()
{
    int n,count=0,i;
    printf("enter n:");
    scanf("%d",&n);
    for(i=1;i<=n;i++)
      if(isprime(i))
         printf("%d ",i);
      printf("\n%d",i);
        
}


//to check whether number is prime or not in boolean value
int main()
{
    int n,count=0,i;
    printf("enter n:");
    scanf("%d",&n);
    printf("\n%d",isprime(n));
        
}





